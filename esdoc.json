{
    "source": "./src",
    "destination": "./docs",
    "includes": [
        "\\.(js|es6)$"
    ],
    "excludes": [
    ],
    "plugins": [{
            "name": "esdoc-es7-plugin"
        },{
            "name": "esdoc-importpath-plugin",
            "option": {
                "replaces": [
                    {"from": "^src/", "to": "lib/"}
                ]
            }
        }
    ]
}
